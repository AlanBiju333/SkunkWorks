<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="SkunkWorks Automation Suite" parallel="classes" thread-count="3">

    <listeners>
        <listener class-name="com.skunkworks.listeners.TestListener"/>
    </listeners>
    <test name="Basic Tests">
        <classes>
            <class name="com.skunkworks.tests.LoginTest"/>
            <class name="com.skunkworks.tests.DashBoardTest"/>
            <class name="com.skunkworks.tests.PIMTest"/>
        </classes>
    </test>
</suite>